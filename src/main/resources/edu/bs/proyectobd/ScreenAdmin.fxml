<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.control.skin.TabPaneSkin.TabContentRegion?>
<AnchorPane xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" fx:controller="edu.bs.proyectobd.controladodor.AdminController" prefWidth="450" prefHeight="350">
    <VBox alignment="CENTER" layoutX="150" layoutY="150">
        <TableView fx:id="usuariosTable" prefWidth="600" prefHeight="400">
            <columns>
                <TableColumn text="Nombre" fx:id="nombreColumn" />
                <TableColumn text="Fecha" fx:id="fechaColumn" />
                <TableColumn text="Cantidad" fx:id="cantidadFotosColumn"/>
                <TableColumn text="Precio" fx:id="precioColumn"/>
            </columns>
        </TableView>

        <Button fx:id="back" onAction="#onRegresarClick" style="-fx-font-size: 18px; -fx-font-family: 'Arial'; -fx-text-fill: #ffffff; -fx-background-color: #773d3d; -fx-padding: 10px 20px; -fx-cursor: hand;" text="Regresar" />
    </VBox>
</AnchorPane>
